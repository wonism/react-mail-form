{
  "name": "react-mail-form",
  "version": "2.1.0",
  "description": "React component for simple contact form with zero dependencies",
  "main": "dist/react-mail-form.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "dev": "NODE_ENV=development webpack-dev-server --config webpack.config.js --hot --host 0.0.0.0",
    "build": "NODE_ENV=production webpack -p --config webpack.config.js",
    "check": "npm run lint && npm t",
    "lint": "eslint lib/**/* || true",
    "start": "npm run dev",
    "test": "jest --bail --coverage --verbose --config jest.config.js",
    "test:watch": "jest --watch --coverage --config jest.config.js"
  },
  "author": "wonism <yocee57@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wonism/react-mail-form.git"
  },
  "bugs": {
    "url": "https://github.com/wonism/react-mail-form/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "react component",
    "contact",
    "contact form",
    "email",
    "react email",
    "react contact",
    "react contact form"
  ],
  "homepage": "https://wonism.github.io",
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
